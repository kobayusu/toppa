<%= form_with(model: @record, local: true) do |form| %>

  <div class="title">
    <%= form.label :text %>
    <%= form.text_field :text %>
  </div>
  
  <div class="time">
    <%= form.label :start_time %>
    <%= form.date_select :start_time %>
  </div>

  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>